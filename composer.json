{
  "name": "spryker/symfony",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "require": {
    "spryker/silex": "^2.0.0",
    "symfony-cmf/routing": "~1.3.0",
    "symfony/console": "~2.8.8",
    "symfony/debug": "~2.8.8",
    "symfony/filesystem": "~2.8.8",
    "symfony/finder": "~2.8.8",
    "symfony/form": "~2.8.8",
    "symfony/http-foundation": "~2.8.8",
    "symfony/http-kernel": "~2.8.8",
    "symfony/intl": "~2.8.8",
    "symfony/options-resolver": "~2.8.8",
    "symfony/process": "~2.8.8",
    "symfony/property-access": "~2.8.8",
    "symfony/routing": "~2.8.8",
    "symfony/security": "~2.8.8",
    "symfony/serializer": "~2.8.8",
    "symfony/translation": "~2.8.8",
    "symfony/twig-bridge": "~2.8.8",
    "symfony/validator": "~2.8.8",
    "symfony/var-dumper": "~2.8.8",
    "symfony/yaml": "~2.8.8"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "description": "Symfony bundle",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
